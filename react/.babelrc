{
  "presets": [
    "env",
    "react",
    [
      "latest",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "imports": "./imports",
          "app": "./app",
          "server": "./server"
        }
      }
    ],
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
